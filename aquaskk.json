						"key_code": "j",
						"modifiers": {
							"mandatory": [
								"left_control"
							]
						}
					},
					"to": [
						{
							"key_code": "j",
							"modifiers": [
								"left_control",
								"left_shift"
							]
						}
					],
					"conditions": [
						{
							"type": "frontmost_application_if",
